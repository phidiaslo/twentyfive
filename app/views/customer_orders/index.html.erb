<p id="notice"><%= notice %></p>

<h1>Listing Customer Orders</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address one</th>
      <th>Address two</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Country</th>
      <th>Email</th>
      <th>Remarks</th>
      <th>Gig</th>
      <th>Buyer</th>
      <th>Seller</th>
      <th>Payment status</th>
      <th>Notification params</th>
      <th>Status</th>
      <th>Transaction</th>
      <th>Purchased at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_orders.each do |customer_order| %>
      <tr>
        <td><%= customer_order.name %></td>
        <td><%= customer_order.address_one %></td>
        <td><%= customer_order.address_two %></td>
        <td><%= customer_order.city %></td>
        <td><%= customer_order.state %></td>
        <td><%= customer_order.zipcode %></td>
        <td><%= customer_order.country %></td>
        <td><%= customer_order.email %></td>
        <td><%= customer_order.remarks %></td>
        <td><%= customer_order.gig_id %></td>
        <td><%= customer_order.buyer_id %></td>
        <td><%= customer_order.seller_id %></td>
        <td><%= customer_order.payment_status %></td>
        <td><%= customer_order.notification_params %></td>
        <td><%= customer_order.status %></td>
        <td><%= customer_order.transaction_id %></td>
        <td><%= customer_order.purchased_at %></td>
        <td><%= link_to 'Show', customer_order %></td>
        <td><%= link_to 'Edit', edit_customer_order_path(customer_order) %></td>
        <td><%= link_to 'Destroy', customer_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer order', new_customer_order_path %>
